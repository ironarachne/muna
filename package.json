{
  "name": "muna",
  "version": "1.3.0",
  "description": "Made-Up Name API",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc --build",
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleDirectories": [
      "<rootDir>/../",
      "<rootDir>/src/",
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "rootDir": ".",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  },
  "keywords": [
    "names"
  ],
  "author": "Ben Overmyer",
  "license": "MIT",
  "dependencies": {
    "@ironarachne/made-up-names": "^1.0.1",
    "hono": "^4.2.4",
    "random": "^3.0.6",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@cloudflare/workers-types": "^4.20240403.0",
    "@jest/globals": "^29.1.1",
    "@types/node": "^18.11.7",
    "@types/random": "^3.0.1",
    "@types/seedrandom": "^3.0.0",
    "@types/service-worker-mock": "^2.0.1",
    "babel-jest": "^29.1.1",
    "jest": "^29.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "wrangler": "^3.45.0"
  }
}
